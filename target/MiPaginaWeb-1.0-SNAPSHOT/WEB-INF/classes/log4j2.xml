<?xml version="1.0" encoding="UTF-8"?>
<Configuation status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <patternLayout pattern="%d{HH:mm:ss} [%t] %-5level %logger{35} - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuation>